        x:Class="HolocronToolset.NET.Dialogs.Save.BifSaveDialog"
        Title="Save in BIF"
        Width="300" Height="150"
        WindowStartupLocation="CenterOwner">
    <StackPanel Margin="10" Spacing="10">
        <Button Name="modSaveButton" Content="Save as MOD" />
        <Button Name="overrideSaveButton" Content="Save as Override" />
        <Button Name="cancelButton" Content="Cancel" />
    </StackPanel>
</Window>
